# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Trailing blank lines
[*]
charset = utf-8
indent_size = 4
indent_style = tab
insert_final_newline = false
trim_trailing_whitespace = true

# package.json specific settings
[{package.json}]
indent_size = 2
indent_style = space
