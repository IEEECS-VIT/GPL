<!DOCTYPE html>
<html>
<head>
    <title>GPL</title>
    <link type="text/css" rel="stylesheet" href="/bower_components/foundation/css/foundation.css"/>
    <link type="text/css" rel="stylesheet" href="/stylesheets/gplmain.css"
    <link href="http://fonts.googleapis.com/css?family=PT+Sans" rel="stylesheet" type="text/css">
    <script src="/javascript/int.js"></script>
    <script src="/bower_components/foundation/js/foundation/foundation.dropdown.js"></script>
    <script src="/bower_components/foundation/js/foundation/foundation.topbar.js"></script>
</head>
<body style="background:url('/images/background.png');font-family: 'PT Sans', sans-serif;height:100%;">
<div style="min-height: 100%">
    <div class="large-12 top-bar" style="overflow: visible;">
        <section class="top-bar-section small-only-text-center">
            <ul class="left">
                <li><a href="#" ="Comming Soon">Gravitas Premier League</a></li>
            </ul>
            <ul class="right">
                <li class="has-dropdown not-click" style="border-radius: 5px">
                    <a href="#" style="border-radius: 5px">Sign In</a>
                    <ul class="dropdown" style="width:300%;">
                        <form>
                            <li class="title back js-generated">
                                <h5>
                                    <a href="javascript:void(0)">
                                        Back
                                    </a>
                                </h5>
                            </li>


                            <li>

                                <div class="small-12 left" style="margin:1%;padding-bottom: 5%">
                                    <div class="row">
                                        <div class="small-3 columns" style="margin-left: -5%">
                                            <label for="team" class="right inline" style="color: white;">Team
                                                Name: </label>
                                        </div>
                                        <div class="small-9 columns">
                                            <input type="text" id="team" name="name" placeholder="Team Name" value=""
                                                   style="margin:-4% 0 0 1%;border-radius: 5px">
                                        </div>
                                    </div>
                                </div>


                                <!--Username/Team Name -->


                                <div class="small-12 left" style="margin:1%;padding-bottom: 5%">
                                    <div class="row" style="margin-top: 2%">
                                        <div class="small-3 columns" style="margin-left: -5%">
                                            <label for="pass" class="right inline"
                                                   style="color: white;">Password: </label>
                                        </div>
                                        <div class="small-9 columns">
                                            <input type="password" id="p
                                                  ass" placeholder="Password" name="password" value=""
                                                   style="margin:-4% 0 0 1%;border-radius: 5px">
                                        </div>
                                    </div>
                                </div>

                                <!-- password-->


                            </li>
                            <li>
                                <input type="button" align="center" class="button [tiny small large]" value="Login"
                                       id="button" name="login"
                                       style="border-radius: 5px;background: rgba(30,30,30,1);margin-left: 38%;margin-bottom: 3%;"/>
                            </li>

                            <li class="divider"><a href="#">Forgot Password?</a></li>
                        </form>
                    </ul>
                </li>
            </ul>
        </section>
    </div>
    <!-- top bar -->
    <div class="row" align="center">

        <div class="small-12 medium-12 large-12 columns"
             style="background:rgba(0,123,184,0.5); border-radius: 0 0 5px 5px;box-shadow: 0 1px 25px 5px rgba(0,0,0,0.5);box-shadow-top:0 0 0 0 rgba(0,0,0,0);float: left;text-align: left ">
            <form name="int" action="/interest" method="post">
                <fieldset>
                    <legend>Player Details</legend>

                    <div class="row">
                        <div class="large-9 small-12">
                            <div class="row">
                                <div class="small-3 columns">
                                    <label for="team" class="right inline" style="color: white;">Name:<i
                                                style="color:red;">*</i> </label>
                                </div>
                                <div class="small-9 columns">
                                    <input type="text" id="team" placeholder="Name" name="name"/>
                                </div>
                            </div>
                        </div>

                        <!--Username/Team Name -->


                        <div class="large-9 small-12">
                            <div class="row">
                                <div class="small-3 columns">
                                    <label for="mob" class="right inline" style="color: white;">Mobile:<i
                                                style="color:red;">*</i> </label>
                                </div>
                                <div class="small-9 columns">
                                    <input type="tel" id="mob" placeholder="9876543210" name="mob"/>
                                </div>
                            </div>
                        </div>
                        <!-- password-->

                        <div class="large-9 small-12">
                            <div class="row">
                                <div class="small-3 columns">
                                    <label for="email" class="right inline" style="color: white;">Email:<i
                                                style="color:red;">*</i> </label>
                                </div>
                                <div class="small-9 columns">
                                    <input type="email" id="email" placeholder="example@domain.com" name="email"/>
                                </div>
                            </div>
                        </div>

                        <div class="large-12" align="center">
                            <input type="submit" id="button" class="button"
                                   style="margin:1%;background: rgba(50,50,50,1);border-radius: 5px" align="center"
                                   name="button"
                                   onmouseover="valid()"/>
                        </div>
                </fieldset>
            </form>
        </div>

    </div>

</div>
<footer class="large-12" style="bottom:0; position:relative;">
    <div class="top-bar" style="position:relative;margin:2% 0 0 0; overflow: hidden;">
        <section class="top-bar-section">
            <ul class="left">
                <li style="display:block;"><a href="#">IEEE-CS VIT Main</a></li>
                <li style="display:block;"><a href="https://vitgravitas.com">graVITas</a></li>
            </ul>
            <ul class="right">

                <li><a href="#">Privacy Policy</a></li>
            </ul>

        </section>
        <p style="margin-left: 46%; font-size:60%;color:white;bottom:0; margin-top:2%">&copy;IEEE-CS VIT Chapter</p>

    </div>
</footer>
<!-- footer -->
</body>
</html>